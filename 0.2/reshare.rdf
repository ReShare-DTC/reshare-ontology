<!-- This is the formal vocabulary description of the ReShare ontology, expressed using W3C RDFS and OWL markup. -->
<!--   ReShare: Reliable Data Sharing Through Digital Transmission Contracts (DTCs) -->
<!--   Todo: Context information about the system -->
<!-- Used namespaces: -->
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:vs="http://www.w3.org/2003/06/sw-vocab-status/ns#"
  xmlns:foaf="http://xmlns.com/foaf/0.1/"
  xmlns:wot="http://xmlns.com/wot/0.1/"
  xmlns:dc="http://purl.org/dc/elements/1.1/">

  <!-- General characteristics of the ReShare ontology -->
  <owl:Ontology rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/" dc:title="ReShare Vocabulary v0.2" dc:description="The ReShare (Reliable Data Sharing Through Digital Transmission contracts) vocabulary, described using W3C RDF Schema and the Web Ontology Language." dc:language="en" owl:versionInfo="0.2" owl:priorVersion="0.1">
  </owl:Ontology>

  <!-- ReShare classes (types) -->
  <rdfs:Class rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#DTC" rdfs:label="Digital Transmission Contract" rdfs:comment="A Digital Transmisson Contract (DTC), used to create data immutability through bilateral signatures." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Identity" rdfs:label="Identity" rdfs:comment="An identity of a transmission party." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Sender" rdfs:label="Sender" rdfs:comment="The data source (sender) of a transmission." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <rdfs:subClassOf rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Identity"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Receiver" rdfs:label="Receiver" rdfs:comment="The receiver of a transmission." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <rdfs:subClassOf rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Identity"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Fact" rdfs:label="Fact" rdfs:comment="A resource checksum which is included in a DTC." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Signature" rdfs:label="Signature" rdfs:comment="A contract signature created by one of the transmission parties." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
  </rdfs:Class>
  <rdf:Property rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#sigData" rdfs:label="signature data" rdfs:comment="The signature data. Should be encoded as base64 using the xsd:base64Binary literal type for reducing encoding overhead." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <rdfs:domain rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Signature"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <!-- The sigData SHOULD be encoded as base64 using the <xsd:base64Binary> literal type for reducing encoding overhead. -->
  </rdf:Property>
  <!-- ReShare properties (relationships) -->
  <rdf:Property rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#sender" rdfs:label="sender" rdfs:comment="The data source (sender) of the underlying transmission of a DTC." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <rdfs:domain rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#DTC"/>
    <rdfs:range rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Sender"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#receiver" rdfs:label="receiver" rdfs:comment="The data receiver of the underlying transmission of a DTC." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <rdfs:domain rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#DTC"/>
    <rdfs:range rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Receiver"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#identifies" rdfs:label="identifies" rdfs:comment="Links an identity of a receiver or sender as part of a contract to that entity. It is best practice to use an instance of foaf:Agent as the object to this property." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <rdfs:domain rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Identity"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#senderSig" rdfs:label="sender signature" rdfs:comment="The contract signature created by the sender of the DTC." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <rdfs:domain rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#DTC"/>
    <rdfs:range rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Signature"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#receiverSig" rdfs:label="receiver signature" rdfs:comment="The contract signature created by the receiver of the DTC." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <rdfs:domain rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#DTC"/>
    <rdfs:range rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Signature"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#fact" rdfs:label="has fact" rdfs:comment="A fact included in a DTC." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <rdfs:domain rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#DTC"/>
    <rdfs:range rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Fact"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#sigType" rdfs:label="signature type" rdfs:comment="The type of a contract signature." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <rdfs:domain rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Signature"/>
    <!-- in the figure, we use a urn:oid:(...) IRI to reference the sigType. It is unclear which class this IRI is mapped to. -->
  </rdf:Property>
  <rdf:Property rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#sigData" rdfs:label="signature data" rdfs:comment="The signature data." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <rdfs:domain rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Signature"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <!-- The sigData SHOULD be encoded as base64 using the <xsd:base64Binary> literal type for reducing encoding overhead. -->
  </rdf:Property>
  <rdf:Property rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#x509Cert" rdfs:label="X509 certificate" rdfs:comment="The X.509 certificate of a transmission party. Should be the full certificate chain,
    excluding the root certificate, encoded as base64 using the xsd:base64Binary literal type. Further encoding is specified in the certEncoding property." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <rdfs:domain rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Identity"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <!-- According to the concept of ReShare, the x509Cert SHOULD be the full certificate chain,
    excluding the root certificate, encoded as base64 using the <xsd:base64Binary> literal type. -->
  </rdf:Property>
  <rdf:Property rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#certEncoding" rdfs:label="X509 certificate encoding" rdfs:comment="Further specification of the X509 certificate encoding. Currently specified are 'X509'/'X509-single' for single certificates in ASN.1 DER notation, and 'PKCS7'/'X509-PKCS7-chain' for a certificate chain encoded as a PKCS#7 message." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <rdfs:domain rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Identity" />
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#factOrigin" rdfs:label="fact origin" rdfs:comment="The resource which is referenced by the fact." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <rdfs:domain rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Fact"/>
    <!-- factOrigin can point at arbitrary IRIs. One SHOULD however check the immutability of the referenced resource,
    or consider using a revisioning system to make referenced resources immutable. -->
  </rdf:Property>
  <rdf:Property rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#sha256Sum" rdfs:label="fact checksum (sha256)" rdfs:comment="A SHA-256 checksum of a fact." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <rdfs:domain rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Fact"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <!-- We do not strictly advise against <xsd:hexBinary> representation of checksums, as a hexstring representation
    is often considered canonical for checksums. A <xsd:base64Binary> representation however reduces encoding overhead.-->
  </rdf:Property>
  <rdf:Property rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#sha384Sum" rdfs:label="fact checksum (sha384)" rdfs:comment="A SHA-384 checksum of a fact." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <rdfs:domain rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Fact"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <!-- We do not strictly advise against <xsd:hexBinary> representation of checksums, as a hexstring representation
    is often considered canonical for checksums. A <xsd:base64Binary> representation however reduces encoding overhead.-->
  </rdf:Property>
  <rdf:Property rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#sha512Sum" rdfs:label="fact checksum (sha512)" rdfs:comment="A SHA-512 checksum of a fact." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <rdfs:domain rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Fact"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <!-- We do not strictly advise against <xsd:hexBinary> representation of checksums, as a hexstring representation
    is often considered canonical for checksums. A <xsd:base64Binary> representation however reduces encoding overhead.-->
  </rdf:Property>
  <rdf:Property rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#serialization" rdfs:label="fact serialization" rdfs:comment="The serialization method used to create a fact checksum. Should be normalized, so that checksum verification is deterministic." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <rdfs:domain rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#Fact"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <!-- Future work: Define a vocabulary for RDF resource serializations (canonical XML, canonical N-Quads, canonical JSON,...)
    As of now, we just use string literals. -->
  </rdf:Property>
  <rdf:Property rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#canonicalization" rdfs:label="fact canonicalization" rdfs:comment="Deprecated equivalent to the fact serialization. Kept for backwards compatibility." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <owl:equivalentProperty rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#serialization" />
  </rdf:Property>
  <rdf:Property rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#customContent" rdfs:label="custom content" rdfs:comment="The property linking a DTC to the custom content included by one of the parties. The object should be a literal with the JSON type (http://www.w3.org/1999/02/22-rdf-syntax-ns#JSON). Abstract property, use the sub-properties instead." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <rdfs:domain rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#DTC"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#senderCustomContent" rdfs:label="sender custom content" rdfs:comment="The sender's custom content." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <rdfs:subPropertyOf rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#customContent" />
    <rdfs:domain rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#DTC"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#receiverCustomContent" rdfs:label="receiver custom content" rdfs:comment="The receiver's custom content." vs:term_status="unstable">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:isDefinedBy rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/"/>
    <rdfs:subPropertyOf rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#customContent" />
    <rdfs:domain rdf:resource="http://i5.pages.rwth-aachen.de/factdag/reshare-ontology/0.2/#DTC"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </rdf:Property>
</rdf:RDF>